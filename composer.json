{
    "name": "wtg/ip-country-detector",
    "description": "A Laravel package for detecting country by IP address",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bohdan Suprun",
            "email": "bohdan-suprun@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "wtg\\IpCountryDetector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "wtg\\IpCountryDetector\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "wtg\\IpCountryDetector\\IpCountryDetectorServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
